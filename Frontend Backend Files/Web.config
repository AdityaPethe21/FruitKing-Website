<?xml version="1.0"?>
<configuration>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.8.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.8.1"/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
	<connectionStrings>
		<add name="ProjectFruitKing" connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=ProjectFruitKIng;Integrated Security=True" providerName="System.Data.SqlCLient"/>
	</connectionStrings>
	<appSettings>
		<add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
	</appSettings>
	<system.net>
		<mailSettings>
			<smtp from="sender Email">
				<network host="smtp.gmail.com" port="587" userName="sender email" password="sender password"/>
			</smtp>
		</mailSettings>
	</system.net>
</configuration>
